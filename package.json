{
  "engines": {
    "node": ">=12.21.0",
    "npm": ">=6.14.11"
  },
  "name": "overhide-ledger",
  "version": "1.0.0",
  "description": "Orchestrate payments between payers and payees known only to this service--not to eachother--but furnishing said payments in this centralized ledger as payments between verifiable pseudonymous parties.",
  "main": "main/js/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JakubNer/overhide-ledger.git"
  },
  "keywords": [
    "overhide",
    "ledger",
    "blockchain",
    "fiat",
    "stripe"
  ],
  "author": "Jakub Ner",
  "license": "MIT",
  "homepage": "https://github.com/JakubNer/overhide-ledger.git",
  "dependencies": {
    "@godaddy/terminus": "4.1.0",
    "@keyv/redis": "1.3.8",
    "applicationinsights": "2.1.4",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "csv-parse": "4.15.3",
    "debug": "4.1.0",
    "ejs": "2.6.1",
    "email-validator": "2.0.4",
    "express": "4.16.4",
    "express-rate-limit": "3.3.2",
    "js-yaml": "3.13.1",
    "keyv": "3.1.0",
    "ledgers.js": "4.6.1",
    "license-compatibility-checker": "^0.3.5",
    "node-fetch": "2.3.0",
    "nodemailer": "5.0.0",
    "pg": "8.7.1",
    "qs": "6.6.0",
    "rate-limit-redis": "2.1.0",
    "stripe": "8.199.0",
    "swagger-jsdoc": "^3.2.9",
    "uuid": "3.3.2",
    "web3": "^1.0.0-beta.55"
  },
  "config": {
    "PROTOCOL": "http",
    "BASE_URL": "localhost:54321",
    "PORT": 54321,
    "DEBUG": "",
    "STRIPE_PUBLISHABLE_KEY": "",
    "STRIPE_SECRET_KEY": "",
    "STRIPE_OAUTH_TOKEN_URL": "https://connect.stripe.com/oauth/token",
    "STRIPE_OAUTH_AUTHORIZE_URL": "https://connect.stripe.com/oauth/authorize",
    "STRIPE_CLIENT_ID": "",
    "STRIPE_MINIMUM_AMOUNT": 0.5,
    "OUR_PROCESSING_FEE_CENTS": "",
    "OUR_PROCESSING_CURRENCY": "USD",
    "OUR_RETARGET_FEE_CENTS": "",
    "RATE_LIMIT_FE_WINDOW_MS": 1000,
    "RATE_LIMIT_FE_MAX_REQUESTS_PER_WINDOW": 100,
    "RATE_LIMIT_FE_REDIS_URI": "",
    "RATE_LIMIT_FE_REDIS_NAMESPACE": "",
    "RATE_LIMIT_BE_WINDOW_MS": 1000,
    "RATE_LIMIT_BE_MAX_REQUESTS_PER_WINDOW": 100,
    "RATE_LIMIT_BE_REDIS_URI": "",
    "RATE_LIMIT_BE_REDIS_NAMESPACE": "",
    "KEYV_TALLY_CACHE_URI": "",
    "POSTGRES_HOST": "localhost",
    "POSTGRES_PORT": 5432,
    "POSTGRES_DATABASE": "",
    "POSTGRES_USER": "",
    "POSTGRES_PASSWORD": "",
    "POSTGRES_SSL": "",
    "SELECT_MAX_ROWS": 30,
    "SALT": "",
    "TOKEN_URL": "https://token.overhide.io/validate",
    "RECAPTCHA_URI": "https://www.google.com/recaptcha/api/siteverify",
    "RECAPTCHA_SITE_KEY": "",
    "RECAPTCHA_SECRET_KEY": "",
    "KEYV_URI": "",
    "KEYV_RETARGET_NAMESPACE": "retarget",
    "KEYV_RETARGET_TTL_MILLIS": "",
    "SMTP_HOST": "",
    "SMTP_PORT": "",
    "SMTP_SECURE": "true",
    "SMTP_USER": "",
    "SMTP_PASSWORD": "",
    "MAILER_FROM": ""
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "copyfiles": "^2.4.1",
    "mocha": "5.2.0",
    "nodemon": "1.18.10",
    "puppeteer": "^1.15.0"
  },
  "scripts": {
    "lic-check": "license-compatibility-checker",
    "start": "node main/js/index.js",
    "test": "mocha test/js",
    "load": "node test/load/load.js",
    "dev": "nodemon --inspect main/js/index.js",
    "cp": "copyfiles --help",
    "pull-ledgers.js": "copyfiles --flat node_modules/ledgers.js/dist/ledgers.js main/static/lib",
    "build": "npm run pull-ledgers.js & docker build -t oh-ledger -f main/docker/Dockerfile .",
    "compose-dev": "docker-compose -f main/docker/docker-compose.yml -f main/docker/docker-compose.dev.yml --project-directory . --project-name oh up -d",
    "psql-dev": "docker run -it --rm --link postgres:postgres --network oh_default postgres psql -h postgres -U adam -d postgres",
    "redis-cli": "docker exec -it redis redis-cli",
    "db-evolve": "node tools/db-evolve/db-evolve.js",
    "db-import-stripe-tx-csv": "node tools/db/db-import-stripe-tx-csv.js",
    "db-import-stripe-retarget-csv": "node tools/db/db-import-stripe-retarget-csv.js"
  }
}
